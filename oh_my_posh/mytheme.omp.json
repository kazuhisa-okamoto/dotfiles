{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#91ddff",
          "foreground": "#100e23",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "options": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "type": "path"
        }, 
        {
          "background": "#95ffa4",
          "foreground": "#193549",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#ff8080",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ reason .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
